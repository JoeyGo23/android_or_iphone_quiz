<h5 class="subtitle"><span class="android">Android</span> vs <span class="iphone">iPhone</span></h5>
<!-- <h4 class="fits">Answer the questions for your Results.</h4> -->
<div class="wrap-question">
  <% @questions.each do |question| %>
    <div class="question" > <%= question.content %> </div>
      <div class="answer">
        <% question.answers.each do |answer| %>
	      <ul>
	      <li><input class="input" type="radio" name=<%= question.id %>, true>
         
         <%= answer.content %>
	      </li>
	      </ul>
        <% end %>
    <% end %>
  </div>
  <input class="submit-button" type=submit>
</div>


